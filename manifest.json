{
    "manifets_version": 2,
    "name": "Block Sites",
    "version": 1.0,
    "permissions": [
        "webRequest",
        "webRequestBlocking", 
        "storage", 
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html" 
    }
}